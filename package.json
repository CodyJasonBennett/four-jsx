{
  "name": "react-nylon",
  "version": "0.0.0",
  "description": "react-reconciler implemented in < 4KB.",
  "keywords": [
    "react",
    "renderers",
    "reconciliation",
    "jsx",
    "hooks",
    "vdom",
    "fiber",
    "minimal"
  ],
  "author": "Cody Bennett (https://github.com/CodyJasonBennett)",
  "homepage": "https://github.com/CodyJasonBennett/react-nylon",
  "repository": "https://github.com/CodyJasonBennett/react-nylon",
  "license": "MIT",
  "files": [
    "dist"
  ],
  "types": "./dist/index.d.ts",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "exports": {
    "types": "./dist/index.d.ts",
    "require": "./dist/index.js",
    "import": "./dist/index.mjs"
  },
  "sideEffects": false,
  "devDependencies": {
    "@types/react": "^18.0.26",
    "microbundle": "^0.15.1",
    "react": "^18.2.0",
    "typescript": "^4.9.3",
    "vite": "^3.2.4"
  },
  "peerDependencies": {
    "react": ">=16.8.0"
  },
  "scripts": {
    "dev": "vite demo",
    "build": "microbundle --raw -f esm,cjs",
    "lint": "tsc"
  },
  "mangle": {
    "regex": "^_"
  },
  "browserslist": [
    "node 16"
  ]
}
